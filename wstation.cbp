<?xml version="1.0" encoding="UTF-8" standalone="yes" ?>
<CodeBlocks_project_file>
	<FileVersion major="1" minor="6" />
	<Project>
		<Option title="wstation" />
		<Option pch_mode="2" />
		<Option compiler="msvc" />
		<Build>
			<Target title="MSVC_Debug">
				<Option output=".cmp/msvc/bin/Debug/wstationd" prefix_auto="1" extension_auto="1" />
				<Option object_output=".cmp/msvc/obj/Debug/" />
				<Option type="1" />
				<Option compiler="msvc" />
				<Compiler>
					<Add option="/MDd" />
					<Add option="/Od" />
					<Add option="/RTCsu" />
					<Add option="/Fd$(TARGET_OUTPUT_DIR)$(TARGET_OUTPUT_BASENAME).pdb" />
					<Add option="/GF" />
					<Add option="/EHsc" />
					<Add option="/W3" />
					<Add option="`$(CPDE_USR)\bin\wx-config.exe --prefix=$(#wx) --wxcfg=$(#wx.debug) --cxxflags`" />
					<Add option="/D_CRT_SECURE_NO_WARNINGS" />
					<Add option="/D_CRT_NONSTDC_NO_DEPRECATE" />
					<Add option="/D_CRT_SECURE_DEPRECATE" />
				</Compiler>
				<Linker>
					<Add option="`$(CPDE_USR)\bin\wx-config.exe  --prefix=$(#wx) --wxcfg=$(#wx.debug) --libs`" />
					<Add option="/NODEFAULTLIB:libcmt.lib" />
					<Add option="/NODEFAULTLIB:msvcrt.lib" />
					<Add option="/INCREMENTAL:NO" />
					<Add library="msvcrtd.lib" />
				</Linker>
			</Target>
			<Target title="MSVC_Release">
				<Option output=".cmp/msvc/bin/Release/wstation" prefix_auto="1" extension_auto="1" />
				<Option object_output=".cmp/msvc/obj/Release/" />
				<Option type="1" />
				<Option compiler="msvc" />
				<Compiler>
					<Add option="/MD" />
					<Add option="/Ox" />
					<Add option="/GF" />
					<Add option="/EHsc" />
					<Add option="/W3" />
					<Add option="`$(CPDE_USR)\bin\wx-config.exe --prefix=$(#wx) --wxcfg=$(#wx.release) --cxxflags`" />
					<Add option="/D_CRT_SECURE_NO_WARNINGS" />
					<Add option="/D_CRT_NONSTDC_NO_DEPRECATE" />
					<Add option="/D_CRT_SECURE_DEPRECATE" />
				</Compiler>
				<Linker>
					<Add option="`$(CPDE_USR)\bin\wx-config.exe  --prefix=$(#wx) --wxcfg=$(#wx.release) --libs`" />
					<Add option="/NODEFAULTLIB:libcmtd.lib" />
					<Add option="/NODEFAULTLIB:msvcrtd.lib" />
					<Add option="/INCREMENTAL:NO" />
					<Add library="msvcrt.lib" />
				</Linker>
			</Target>
			<Target title="GCC_Debug">
				<Option output=".cmp/gcc/bin/Debug/wstationd" prefix_auto="1" extension_auto="1" />
				<Option object_output=".cmp/gcc/obj/Debug/" />
				<Option type="1" />
				<Option compiler="gcc_generic" />
				<Compiler>
					<Add option="-std=c++0x" />
					<Add option="-W" />
					<Add option="-fPIC" />
					<Add option="-fexceptions" />
					<Add option="-g" />
					<Add option="`$(#wx.config) --cxxflags --debug`" />
					<Add option="-DNOPCH" />
					<Add option="-D_DEBUG" />
				</Compiler>
				<Linker>
					<Add option="`$(#wx.config) --libs std,aui --static --debug`" />
					<Add library="ck_sqlite3d" />
					<Add library="cf_utilsd" />
					<Add library="usb" />
				</Linker>
			</Target>
			<Target title="GCC_Release">
				<Option output=".cmp/gcc/bin/Release/wstation" prefix_auto="1" extension_auto="1" />
				<Option object_output=".cmp/gcc/obj/Release/" />
				<Option type="1" />
				<Option compiler="gcc_generic" />
				<Option projectLinkerOptionsRelation="2" />
				<Compiler>
					<Add option="-std=c++0x" />
					<Add option="-W" />
					<Add option="-fPIC" />
					<Add option="-fexceptions" />
					<Add option="-Os" />
					<Add option="`$(#wx.config) --cxxflags`" />
					<Add option="-DNOPCH" />
				</Compiler>
				<Linker>
					<Add option="`$(#wx.config) --libs std,aui --static`" />
					<Add library="ck_sqlite3" />
					<Add library="cf_utils" />
					<Add library="usb" />
				</Linker>
				<ExtraCommands>
					<Add before="rm -f $(TARGET_OUTPUT_FILE)" />
					<Add after="upx $(TARGET_OUTPUT_FILE)" />
					<Add after="$(CPDE_USR)/bin/cpde_usr -bin -xi -project=$(PROJECT_NAME)  -root=$(PROJECT_DIR)  -build=$(TARGET_NAME)  -target=$(TARGET_OUTPUT_FILE)  -usr=$(CPDE_USR)" />
					<Mode after="always" />
				</ExtraCommands>
			</Target>
		</Build>
		<Compiler>
			<Add directory="$(CPDE_USR)/include" />
			<Add directory="$(#boost.include)" />
		</Compiler>
		<Linker>
			<Add directory="$(CPDE_USR)/lib" />
			<Add directory="$(#boost.lib)" />
		</Linker>
		<Unit filename="RunningAverage.cpp" />
		<Unit filename="RunningAverage.h" />
		<Unit filename="main.cpp" />
		<Unit filename="sqlTextExporter.cpp" />
		<Unit filename="sqlTextExporter.h" />
		<Unit filename="sqlWeatherStation.cpp" />
		<Unit filename="sqlWeatherStation.h" />
		<Unit filename="wsMeasurement.cpp" />
		<Unit filename="wsMeasurement.h" />
		<Unit filename="wsp/wsp.c">
			<Option compilerVar="CC" />
		</Unit>
		<Unit filename="wsp/wsp_plus.h" />
		<Extensions>
			<code_completion />
			<envvars />
			<debugger />
			<lib_finder disable_auto="1" />
		</Extensions>
	</Project>
</CodeBlocks_project_file>
